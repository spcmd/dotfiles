" Create a code block named clearTweetdeck
clearTweetdeck -> {{
  // We are now in JavaScript mode
  javascript:(function(){c=document.getElementsByClassName('js-chirp-container');for(i=0;i<c.length;i++)c[i].innerHTML=""})();
}}

" Run the JavaScript block
map C :call clearTweetdeck<CR>
