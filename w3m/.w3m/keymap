
# The 'M' modifier key is the Esc key!

# Basic stuff
keymap p OPTIONS
keymap q QUIT
keymap : GOTO
keymap F TAB_LINK
keymap T TAB_LINK
keymap r RELOAD
keymap f LIST_MENU

# Open link in external (GUI) browser
keymap o EXTERN_LINK

# Open current page in external (GUI) browser
keymap O EXTERN

# Move the cursor
keymap	h	MOVE_LEFT
keymap	j	MOVE_DOWN
keymap	k	MOVE_UP
keymap	l	MOVE_RIGHT

keymap	b	PREV_WORD
keymap	w	NEXT_WORD

# Move (without moving the cursor)
#keymap	J	UP
#keymap	K	DOWN
keymap	g	BEGIN
keymap	G	END
keymap	J	NEXT_PAGE
keymap	K	PREV_PAGE

# Tabs
keymap t NEW_TAB
keymap d CLOSE_TAB
keymap , PREV_TAB
keymap . NEXT_TAB

# Bookmark
keymap a ADD_BOOKMARK
keymap C-b BOOKMARK

# Buffers and History
keymap	H	BACK
keymap	B	BACK
keymap	s	SELECT_MENU
keymap C-h HISTORY

# Search
keymap	/	SEARCH
keymap	?	SEARCH_BACK
keymap	n	SEARCH_NEXT
keymap	N	SEARCH_PREV

# Peek / Info
keymap	c	PEEK
keymap	u	PEEK_LINK
keymap	i	PEEK_IMG
keymap	I	INFO
keymap	C	COOKIE


